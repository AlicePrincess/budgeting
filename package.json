{
  "name": "webpack-react-redux-budget-app-sample",
  "version": "2.0.0",
  "private": true,
  "description": "Budgeting App Sample - React, Redux, Webpack",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --config webpack.config.prod.js",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "serve": "pushstate-server build/ 3000",
    "perf": "rimraf *.report.dom.html && npm run build && (npm run serve & lighthouse http://localhost:3000 --view)",
    "perf-live": "lighthouse https://budget.modus.app --view",
    "deploy-github": "npm run build && gh-pages -d build",
    "lint": "eslint ./app -f table",
    "lint:fix": "eslint ./app -f table --fix",
    "update-types": "flow-typed install",
    "flow": "flow",
    "dll": "webpack --config webpack.config.dll.js",
    "postinstall": "rimraf .cache-loader & npm run dll"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.3",
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "cache-loader": "1.2.2",
    "coveralls": "2.13.3",
    "cross-env": "5.0.5",
    "css-loader": "0.28.11",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "1.1.11",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.2",
    "lighthouse": "2.9.4",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "postcss-loader": "2.1.5",
    "prettier": "1.6.1",
    "pushstate-server": "3.0.1",
    "react-test-renderer": "16.0.0-beta.5",
    "redux-mock-store": "^1.2.3",
    "rimraf": "2.6.1",
    "sass-loader": "7.0.1",
    "script-ext-html-webpack-plugin": "2.0.1",
    "style-loader": "0.21.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglify-es": "3.3.9",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-parallel-uglify-plugin": "1.1.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "d3": "4.10.2",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "reselect": "^3.0.1"
  }
}
